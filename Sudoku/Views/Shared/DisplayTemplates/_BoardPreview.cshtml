@model char[,]

@for (int i = 0; i < 3; i++)
{
    for (int j = 0; j < 3; j++)
    {
        <div class="game-preview-grid-3by3">
        @for (int row = 0; row < 3; row++)
        {
            for (int col = 0; col < 3; col++)
            {
                int r = i * 3 + row;
                int c = j * 3 + col;
                    
                <div class="game-preview-square">
                    <div class="game-preview-square-content">
                        @Model[r, c]
                    </div> 
                </div>
            }
        }
        </div>
    }
}

